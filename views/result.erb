<a href="/">Home</a>

<%= @song.inspect %>
<div class="content">
	<div class="columns">
		<div class="column is-one-half">
			<ul>
				<li>Artist: <%= @song.artist %></li>
				<li>Title: <%= @song.title %></li>
				<li>ISRC: <%= @song.isrc %></li>
				<li>Release: <%= @song.release %></li>
				<li>Danceability: <%= @song.danceability %></li>
				<li>Duration: <%= @song.duration %></li>
				<li>Energy: <%= @song.energy %></li>
				<li>Loudness: <%= @song.loudness %></li>
				<li>Musical Key: <%= @song.musical_key %></li>
				<li>Tempo: <%= @song.tempo %></li>
				<li>Time Signature: <%= @song.time_signature %></li>
			</ul>
		</div>
	</div>
</div>
